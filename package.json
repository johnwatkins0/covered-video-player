{
  "name": "covered-video-player",
  "version": "0.0.3",
  "description":
    "An HTML5 video player with any HTML superimposed as a play button.",
  "main": "src/index.js",
  "repository": "https://github.com/johnwatkins0/covered-video-player.git",
  "scripts": {
    "start":
      "parcel example/src/index.html -d example -o --public-url /covered-video-player/example/ &",
    "build":
      "parcel build example/src/index.html -d example --public-url /covered-video-player/example/",
    "lint": "eslint src",
    "prepare": "npm run build",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "author": "John Watkins",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^8.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.3.0",
    "eslint": "^4.14.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "jest": "^22.0.4",
    "parcel-bundler": "^1.3.1",
    "puppeteer": "^0.13.0"
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  }
}
